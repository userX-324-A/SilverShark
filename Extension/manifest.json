{
  "manifest_version": 3,
  "name": "SilverShark",
  "version": "1.0",
  "description": "An extension to read and interact with the Horizon XE website.",
  "permissions": [
    "activeTab",
    "scripting",
    "nativeMessaging",
    "storage"
  ],
  "content_scripts": [
    {
      "matches": ["*://*/HORIZON/xe-app/index.html"],
      "js": ["content.js"]
    }
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/icon16.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png"
    }
  },
  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  }
} 